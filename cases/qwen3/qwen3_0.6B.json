{        
    "version":"0.1.0",     
    "# quantizer":{
        "self_attn": {"bits":3},
        "mlp":  {"bits":4},
        "embed_tokens": {"bits":4},
        "#MINI":"off"
    },    
    "model":{   
        "#hf-card":"/home/cys/rnd/lic/Models/Qwen3-0.6B/",
        "arch":"QWEN3",
        "parameter":{
            "Layer": 28,  
            "transformer":  {
                "Ctx":1024,     "Embed":  1024,     "Ffn":3072,
                "Head": 16,     "KVHead": 8,        "head_dim": 128          
            },
            "max_pos_embeddings":32768
        },                  
        "embed_tokens": {"Embedding":[]},    
        "layer":  {      
            "self_attn":{"QKV":[]},               
            "mlp":{"FFN":[ ]},       
            "# gattn":{"GAU":[]}
                                  
        },
        "norm":{"Normal":[]},
        "output": {"CLASIFY":[]}
    },
    
    "sparsing":{
        "activation":"ffn qkv",
        "model":"/home/cys/QWEN_sparse_"
    },
    
    
    "train": {
        "dump-every":10,
        
        "gpt-every":-10,
        "epoch":1,
        "batch":32,
        "learning-rate":0.0006, 

        "optimizatioin":{
            "#method":"adamw sgdv hsgd sgd muon",
            "method":"adamw",
            "sign":0,
            "grad_accumulation":1,
            "lars_ratio":0,      "ZMUV_ratio":0.00    
        }              
    },  
    "datasets":{
        "#train":{
            "glob":"./Datasets/tiny-shakespeare/*train*.bin","most":50,"name":"shake"
        },
        "train":{
            "glob":"./Datasets/climb-1b/*train*.bin","most":50,"name":"climb"
        },
        
        "eval_1": {"glob":"./Datasets/climb-1b/*val*.bin","name":"climb","most":10,"eval-every":500 ,"samp":0.1       },
        "#eval_2":{"glob":"./Datasets/hellaswag_val.bin", "type":"hellaswag","eval-every":500 ,"samp":0.04    }
    },
    "dump": {
        "train_time" : 0,
        "# tensor_ref" : 1,
        "most_mem_item" : 6
    },
    "checkpoint_out":{
        "QWen3_0.6B":{
            "path": "./hy-tmp/checkpoint/",            
            "#type":"best full state",
            "type":"full",       
            "save-every":500,
            "snapshot":{
                "curFuyou": -1
            }
        }
    },
    
    "seed":42
}
