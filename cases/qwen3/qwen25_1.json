{        
    "version":"0.1.0",     
    "# quantizer":{
        "self_attn": {"bits":3},
        "mlp":  {"bits":4},
        "embed_tokens": {"bits":4},
        "#MINI":"off"
    },

    "model":{   
        "hf-card":"./Models/Qwen2.5-0.5B/",
        "parameter":{
            "Layer": 24,  
            "transformer":  {
                "Ctx":1024,     "Embed":  896,     "Ffn":4864,
                "Head": 14,     "KVHead": 2,        "head_dim": 64          
            },
            "max_pos_embeddings":32768
        },         
        "backbone":{             
            "embed_tokens": {"Embedding":[]},    
            "layer":  {      
                "self_attn":{"QKV":[]},               
                "mlp":{"FFN":[ ]},       
                "# gattn":{"GAU":[]}
                                    
            },
            "norm":{"Normal":[]},
            "output": {"CLASIFY":[]}
        }
    },    

    "#test":"GGUF_list GPT_work fish_1 bubble tutor ppl",      

    "gpt":{
        "max_seq_len": 512,
        "c_graph":"raw0",
        "prompt":"when the smoke is going down,"
    },
    
    "sparsing":{
        "activation":"ffn qkv",
        "model":"/home/cys/QWEN_sparse_"
    },
    
    
    "train": {
        "save-every":500,
        "dump-every":1,
        
        "gpt-every":-10,
        "epoch":1,
        "batch":2,
        "learning-rate":1.0e-5,  

        "optimizatioin":{
            "#method":"adamw sgdv hsgd sgd",
            "method":"adamw",
            "sign":0,
            "grad_accumulation":1,
            "lars_ratio":0,      "ZMUV_ratio":0.00    
        }              
    },  
    "datasets":{
        "train":{
            "glob":"./Datasets/tiny-shakespeare/*train*.bin","most":50,"name":"shake"
        },
        
        "#eval_1": {"glob":"./Datasets/tiny-shakespeare/*val*.bin","name":"shake","most":10,"eval-every":200 ,"samp":0.1       },
        "#eval_2":{"glob":"./Datasets/hellaswag_val.bin", "type":"hellaswag","eval-every":500 ,"samp":0.04    }
    },
    "debug": {
        "prompts":[
        "hello","What is the capital of Shanghai?","Who wrote the play Romeo and Juliet?","In which year did the Titanic sink?",
        "What is the chemical symbol for the element gold?","What is the longest river in the world?",
        "Sally (a girl) has 3 brothers. Each brother has 2 sisters. How many sisters does Sally have?",
        "How many games did Arsenal FC go unbeaten during the 2003-2004 season of the English Premier League",
        "I get out on the top floor (third floor) at street level. How many stories is the building above the ground?",
        "天命玄鸟,降而生生. 玄鸟是什么鸟?"
        ],
        "#prompts":[
            "hello"
        ],
        "most_iter":-25,
        "eval_samp":0.04,
        "#long_tail":10
    },
    "dump": {
        "train_time" : 0,
        "# tensor_ref" : 1,
        "most_mem_item" : 6
    },
    
    "seed":42
}
