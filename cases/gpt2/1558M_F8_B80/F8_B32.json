{
    "version": "0.1.0",
    "model": {
        "arch": "GPT2",
        "fuyou": {
            "branch": 8,
            "switch": 100,
            "#method": [
                "pso",
                "mix",
                "ga",
                "pso_ga"
            ],
            "method": "pso_ga",
            "crossover": 0.6,
            "mutation": 0.001,
            "social": 2
        },
        "datatype": {
            "weight": "BF16",
            "embed": "BF16",
            "#gradient": "BF16",
            "#ternary": [
                "attn.wq.weight",
                "attn.wk.weight",
                "attn.wv.weight",
                "attn.wo.weight",
                "ffn_down.weight",
                "ffn_up.weight"
            ],
            "#tile": []
        },
        "parameter": {
            "Layer": 48,
            "transformer": {
                "Ctx": 1024,
                "Embed": 1600,
                "Head": 25,
                "Ffn": 6400
            }
        },
        "inp_embd": {
            "Embedding+": []
        },
        "Layer": {
            "attn": {
                "QKV": []
            },
            "ffn": {
                "FFN": []
            },
            "# gattn": {
                "GAU": []
            }
        },
        "output_norm": {
            "Normal": []
        },
        "out": {
            "CLASIFY": []
        }
    },
    "datasets": {
        "train": {
            "glob": "./Datasets/edu_fineweb1B/*train*.bin",
            "most": 50,
            "name": "edu_fineweb1B"
        },
        "eval_1": {
            "glob": "./Datasets/edu_fineweb1B/*val*.bin",
            "name": "edu_fineweb1B",
            "most": 1,
            "eval-every": 200,
            "samp": 0.06
        },
        "#eval_2": {
            "glob": "./Datasets/hellaswag_val.bin",
            "type": "hellaswag",
            "eval-every": 1000
        }
    },
    "train": {
        "dump-every": 10,
        "gpt-every": -10,
        "epoch": 1,
        "batch": 32,
        "learning-rate": 0.0006,
        "decay": 0.1,
        "optimizatioin": {
            "#method": "adamw sgdv hsgd lion adams",
            "method": "adamw",
            "sign": 0,
            "grad_accumulation": 1,
            "lars_ratio": 0,
            "ZMUV_ratio": 0.0
        }
    },
    "checkpoint": {
        "save-every": -500,
        "# in": "./hy-tmp/checkpoint/chk-GPT2_9001.gguf",
        "out": "./hy-tmp/checkpoint/GPT2_1558M_"
    },
    "threads": 20,
    "seed": 42
}