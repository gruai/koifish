{
    "version": "0.1.0",
    "model": {
        "arch": "GPT2",
        "fuyou": {
            "branch": 6,
            "switch": 100,
            "#method": [
                "pso",
                "mix",
                "ga",
                "pso_ga"
            ],
            "method": "pso_ga",
            "crossover": 0.6,
            "mutation": 0.001,
            "social": 2
        },
        "datatype": {
            "weight": "BF16",
            "embed": "BF16",
            "#gradient": "BF16",
            "#ternary": [
                "ffn_down.weight",
                "ffn_up.weight"
            ]
        },
        "parameter": {
            "Layer": 12,
            "transformer": {
                "Ctx": 1024,
                "Embed": "768",
                "Head": 12
            }
        },
        "#parameter": {
            "Layer": 48,
            "transformer": {
                "Ctx": 1024,
                "Embed": 1600,
                "Head": 25,
                "Ffn": 6400
            }
        },
        "inp_embd": {
            "Embedding+": []
        },
        "layer": {
            "attn": {
                "QKV": []
            },
            "ffn": {
                "FFN": []
            },
            "# gattn": {
                "GAU": []
            }
        },
        "output_norm": {
            "Normal": []
        },
        "out": {
            "CLASIFY": []
        }
    },
    "datasets": {
        "train": {
            "glob": "./Datasets/edu_fineweb1B/*train*.bin",
            "most": 50,
            "name": "edu_fineweb1B"
        },
        "eval_1": {
            "glob": "./Datasets/edu_fineweb1B/*val*.bin",
            "name": "edu_fineweb1B",
            "most": 10,
            "eval-every": 200,
            "samp": 0.1
        },
        "#eval_2": {
            "glob": "./Datasets/hellaswag_val.bin",
            "type": "hellaswag",
            "eval-every": 500,
            "samp": 0.04
        }
    },
    "train": {
        "dump-every": 10,
        "gpt-every": -10,
        "# batch_sample": "super",
        "# sample": 0.1,
        "epoch": 1,
        "batch": 80,
        "learning-rate": 0.0006,
        "decay": 0.1,
        "optimizatioin": {
            "#method": "adamw sgdv hsgd sgd lion adams muon",
            "method": "adamw",
            "sign": 0,
            "grad_accumulation": 1,
            "lars_ratio": 0,
            "ZMUV_ratio": 0.0
        }
    },
    "debug": {
        "#x_str": "blk.2.attn.wq.weight",
        "most_iter": -25
    },
    "dump": {
        "train_time": 0,
        "most_mem_item": 6
    },
    "checkpoints": {
        "#in": "./hy-tmp/checkpoint/GPT2_144M.fuyou",
        "#out": {
            "path": "./hy-tmp/checkpoint/GPT2_fuyou_",
            "#type": "best full state",
            "type": "best",
            "save-every": 500
        }
    },
    "seed": 42
}