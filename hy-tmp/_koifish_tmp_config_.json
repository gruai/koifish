{
    "version": "0.1.0",
    "model": {
        "arch": "GPT2",
        "fuyou": {
            "branch": 9,
            "switch": 100,
            "method": "pso_ga",
            "crossover": 0.6,
            "mutation": 0.001,
            "social": 2
        },
        "datatype": {
            "weight": "BF16",
            "embed": "BF16"
        },
        "parameter": {
            "Layer": 36,
            "transformer": {
                "Ctx": 1024,
                "Embed": 1280,
                "Head": 20,
                "Ffn": 5120
            }
        },
        "inp_embd": {
            "Embedding+": []
        },
        "Layer": {
            "attn": {
                "QKV": []
            },
            "ffn": {
                "FFN": []
            }
        },
        "output_norm": {
            "Normal": []
        },
        "out": {
            "CLASIFY": []
        }
    },
    "datasets": {
        "train": {
            "glob": "./Datasets/edu_fineweb1B/*train*.bin",
            "most": 50,
            "name": "edu_fineweb1B"
        },
        "eval_1": {
            "glob": "./Datasets/edu_fineweb1B/*val*.bin",
            "name": "edu_fineweb1B",
            "most": 10,
            "eval-every": 200,
            "samp": 0.04
        }
    },
    "train": {
        "save-every": 500,
        "dump-every": 10,
        "gpt-every": -10,
        "epoch": 1,
        "batch": 40,
        "learning-rate": 0.001,
        "decay": 0.1,
        "optimizatioin": {
            "method": "adamw",
            "sign": 0,
            "grad_accumulation": 1,
            "lars_ratio": 0,
            "ZMUV_ratio": 0.0
        }
    },
    "checkpoint-out": "./hy-tmp/checkpoint/GPT2_774M_",
    "threads": 20,
    "seed": 42,
    "debug": {
        "most_iter": 10
    },
    "checkpoint_in": null
}